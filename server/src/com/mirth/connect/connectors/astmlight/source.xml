<connectorMetaData path="astmlight">
	<name>ASTM Light Listener</name>
	<author>NextGen Healthcare</author>
	<pluginVersion>@mirthversion</pluginVersion>
	<mirthVersion>3.9.0</mirthVersion>
	<url>http://www.nextgen.com</url>
	<description>This connector allows Mirth Connect to listen for incoming ASTM messages over a standard TCP connection with basic message handling.</description>
	<clientClassName>com.mirth.connect.connectors.tcp.TcpListener</clientClassName>
	<serverClassName>com.mirth.connect.connectors.tcp.TcpReceiver</serverClassName>
	<sharedClassName>com.mirth.connect.connectors.tcp.TcpReceiverProperties</sharedClassName>
	<library type="CLIENT" path="astmlight-client.jar" />
	<library type="SHARED" path="astmlight-shared.jar" />
	<library type="SERVER" path="astmlight-server.jar" />
	<transformers/>
	<protocol>astm</protocol>
	<type>SOURCE</type>
</connectorMetaData>